# HPE Aruba Networking Fabric Composer Configuration
# Copy this file to .env and fill in your credentials

# Fabric Composer Connection (REQUIRED)
FABRIC_COMPOSER_IP=172.31.8.99
FABRIC_COMPOSER_USERNAME=admin
FABRIC_COMPOSER_PASSWORD=your_password_here

# Fabric Composer Connection Details (OPTIONAL)
FABRIC_COMPOSER_PORT=443
FABRIC_COMPOSER_PROTOCOL=https

# Token Management (OPTIONAL)
# Refresh token this many seconds before expiry (default: 300 = 5 minutes)
TOKEN_REFRESH_MARGIN=300

# Logging Configuration (OPTIONAL)
# Comma-separated list of log levels to display
# Available levels: debug, info, notice, warn, err, crit, alert, emerg
LOG_VIEW=info,notice,warn,err,crit,alert,emerg
